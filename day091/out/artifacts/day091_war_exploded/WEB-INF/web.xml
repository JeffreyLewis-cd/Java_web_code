<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <!--servlet01-->
    <servlet>
        <servlet-name>Servlet_Demo_Test01</servlet-name>
        <servlet-class>Servlet_Demo</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet_Demo_Test01</servlet-name>
        <url-pattern>/Servlet_Demo/urlTest01</url-pattern>


    </servlet-mapping>

    <!--servlet02-->
    <servlet>
        <servlet-name>Servlet_Demo_Test02</servlet-name>
        <servlet-class>Servlet_Demo</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>Servlet_Demo_Test02</servlet-name>
        <url-pattern>/Servlet_Demo/urlTest02</url-pattern>
    </servlet-mapping>

</web-app>